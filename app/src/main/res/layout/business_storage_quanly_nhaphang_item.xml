<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="100dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv_storage_quanlynhaphang_manhap"
        android:text="Mã nhập"
        android:textStyle="bold"
        android:textSize="20dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv_storage_quanlynhaphang_sosp"
        android:text="Số sản phẩm nhập"
        app:layout_constraintTop_toBottomOf="@id/tv_storage_quanlynhaphang_manhap"
        app:layout_constraintStart_toStartOf="@id/tv_storage_quanlynhaphang_manhap"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv_storage_quanlynhaphang_ngaynhap"
        android:text="DD/MM/YYYY"
        app:layout_constraintTop_toTopOf="@id/tv_storage_quanlynhaphang_manhap"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="10dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv_storage_quanlynhaphang_tongtien"
        android:text="Tổng tiền"
        app:layout_constraintTop_toBottomOf="@id/tv_storage_quanlynhaphang_sosp"
        app:layout_constraintStart_toStartOf="@id/tv_storage_quanlynhaphang_manhap"/>
</androidx.constraintlayout.widget.ConstraintLayout>